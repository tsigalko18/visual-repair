			assertEquals("GBP", uk.toString());
			assertEquals("GBP", uk.getCurrencyCode());
			assertEquals("GBP", uk.getSymbol());
			assertEquals(us.getDefaultFractionDigits(), uk.getDefaultFractionDigits());
		}
	}
	
